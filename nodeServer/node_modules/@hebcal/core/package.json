{
  "name": "@hebcal/core",
  "version": "5.4.11",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "contributors": [
    "Eyal Schachter (https://github.com/Scimonster)",
    "Danny Sadinoff (https://github.com/dsadinoff)",
    "Benny Powers (https://github.com/bennypowers)"
  ],
  "keywords": [
    "hebcal",
    "hebrew",
    "calendar",
    "jewish",
    "holidays",
    "chagim",
    "sedra",
    "date",
    "dafyomi",
    "zemanim",
    "zmanim"
  ],
  "description": "A perpetual Jewish Calendar API",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "type": "module",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "typings": "./dist/index.d.ts",
  "engines": {
    "node": ">= 16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebcal/hebcal-es6.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-es6/issues"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:rollup": "rollup -c",
    "build": "npm run po2json && npm run version && npm run build:rollup --production",
    "prepublish": "npm run build",
    "version": "node ./version.cjs package.json src/pkgVersion.ts",
    "po2json": "node ./po2json.cjs po/*.po",
    "docs": "typedoc",
    "pretest": "npm run build",
    "lint": "gts lint",
    "coverage": "jest --coverage",
    "test": "jest",
    "clean": "gts clean",
    "fix": "gts fix"
  },
  "license": "GPL-2.0",
  "overrides": {
    "babel-plugin-ttag@1.8.16": {
      "babel-plugin-macros": "3.1.0"
    }
  },
  "peerDependencies": {
    "temporal-polyfill": "^0.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/register": "^7.24.6",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "core-js": "^3.37.1",
    "gts": "^5.3.1",
    "jest": "^29.7.0",
    "quick-lru": "<=6.1.2",
    "rollup": "^4.18.1",
    "ttag-cli": "^1.10.12",
    "typedoc": "^0.26.4",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@hebcal/hdate": "^0.11.4",
    "@hebcal/noaa": "^0.8.14",
    "tslib": "^2.6.3"
  }
}
